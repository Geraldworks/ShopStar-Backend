{
  "name": "shopstar-backend",
  "version": "1.0.0",
  "description": "backend for shopstar",
  "main": "src/index.js",
  "scripts": {
    "docker:test": "docker compose -f test.docker-compose.yml up -d",
    "prisma-migrate:dev": "dotenv -e .env.dev -- npx prisma migrate dev --schema prisma/schema.prisma",
    "prisma-migrate:test": "dotenv -e .env.test -- npx prisma db push --schema prisma/schema.test.prisma",
    "prisma-studio:dev": "dotenv -e .env.dev -- npx prisma studio --schema prisma/schema.prisma",
    "prisma-studio:test": "dotenv -e .env.test -- npx prisma studio --schema prisma/schema.test.prisma",
    "prettier:write": "prettier --write .",
    "prettier:check": "prettier --check .",
    "dev": "NODE_ENV=dev ts-node-dev src/index.ts",
    "test": "NODE_ENV=test npm run docker:test && npm run prisma-migrate:test && dotenv -e .env.test jest",
    "init-db:dev": "npm run prisma-migrate:dev && dotenv -e .env.dev -- ts-node src/utils/initialiseDb.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.5.0",
    "jest": "^29.7.0",
    "prettier": "3.3.3",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^5.17.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "@types/bcrypt": "^5.0.2",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.3.0"
  }
}
